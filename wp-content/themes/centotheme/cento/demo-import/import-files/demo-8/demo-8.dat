	line-height: 20px;
}

.wpcf7-form-control.wpcf7-textarea.wpcf7-validates-as-required {
	height: 150px;
}

.wpcf7-form-control.wpcf7-text.wpcf7-validates-as-required:focus {
	background: transparent;
}

